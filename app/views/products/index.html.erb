<div class="container navplace">
   <div class="row">
      <!-- p id="notice"><%= notice %></p -->
      <div class="col-md-6 col-xs-6">
         <div>
            <h1>Products</h1>
         </div>
      </div>
      <div class="col-md-6 col-xs-6">
         <div class="h1 text-right">
           <% if signed_in? && current_user.admin? %>
             <%= link_to 'New Product', new_product_path, class: "btn greenbtn" %> 
             <% end %>  
         </div>
      </div>
      </div>
      <div class="row">
        <% cache(cache_key_for_products) do %> 
        <% @products.each do |product| %>  
            <%= render product %>
          <% end %>                      
        <% end %>
      </div>
      <hr>
</div>
<br>